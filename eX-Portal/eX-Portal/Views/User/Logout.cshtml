@{
  Layout = "~/Views/Shared/_eXPortalBeforeLogin.cshtml";
}


<div>
Thank you for using the portal. 
</div>
<div>
  Please @Html.ActionLink("Log in", "Index", "User") again to use the site.

</div>

<script type="text/javascript">
  
    history.pushState(null, null, 'Logout');
    window.addEventListener('popstate', function (event) {
        history.pushState(null, null, 'Logout');
    });
   
</script>


   