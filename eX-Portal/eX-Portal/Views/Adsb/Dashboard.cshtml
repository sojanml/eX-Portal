
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_ADSB.cshtml";
}


<section class="top-space">
  <div class="column column-half">

    <table style="width:100%;" class="" cellpadding="0" cellspacing="0">
      <tr>
        <td valign="top" class="shaded">
          <div class="title"><b>Tracking</b> (Select Receving Options)</div>
          <ul class="simple-list">
            <li><input checked type="checkbox" name="tracking_adsb_commercial" id="tracking_adsb_commercial" /><label for="tracking_adsb_commercial">ADSB Commercial</label> </li>
            <li><input checked type="checkbox" name="tracking_adsb_rpas" id="tracking_adsb_rpas" /><label for="tracking_adsb_rpas">ADSB RPAS</label></li>
            <li><input type="checkbox" name="tracking_adsb_skycommander" id="tracking_adsb_skycommander" /><label for="tracking_adsb_skycommander">Skycommander RPAS</label></li>
          </ul>
          <div class="hr"></div>
          <div class="title">
            <b>ATC Radius</b> <input class="spinner"
                                     type="text" id="adsb_radious" name="adsb_radious" /> KM
          </div>
          <ul class="multi-select">
            <li><input checked type="checkbox" name="adsb-omdb" id="adsb-omdb" /><label for="adsb-omdb">OMDB</label> </li>
            <li><input checked type="checkbox" name="adsb-omdv" id="adsb-omdv" /><label for="adsb-omdv">OMDV</label></li>
            <li><input type="checkbox" name="adsb-omsj" id="adsb-omsj" /><label for="adsb-omsj">OMDJ</label></li>
          </ul>
        </td>
        <td class="spacer"></td>
        <td valign="top"  class="shaded">
          <div class="title"><b>Traffic STATS &amp; Analysis Dashoard</b>  (Input Parameters)</div>

          <table class="traffic_status" cellpadding="0" cellspacing="0">
            <tr>
              <td rowspan="2"><div class="alert traffic_stat"></div> </td>
              <td>Define Horizondal Seperation</td>
              <td><input id="adsb_safe_h" class="spinner" name="adsb_safe_h" type="text" /></td>
              <td>KM</td>
            </tr>
            <tr>
              <td>Define Vertical Seperation</td>
              <td><input id="adsb_safe_v" class="spinner" name="adsb_safe_v" type="text" /></td>
              <td>KM</td>
            </tr>
            <tr>
              <td rowspan="2"><div class="safe traffic_stat"></div></td>
              <td>Define Horizondal Seperation</td>
              <td><input id="adsb_alert_h" class="spinner" name="adsb_alert_h" type="text" /></td>
              <td>KM</td>
            </tr>
            <tr>
              <td>Define Vertical Seperation</td>
              <td><input id="adsb_alert_v" class="spinner" name="adsb_alert_v" type="text" /></td>
              <td>KM</td>
            </tr>
            <tr>
              <td rowspan="2"><div class="breach traffic_stat"></div></td>
              <td>Define Horizondal Seperation</td>
              <td><input id="adsb_breach_h" class="spinner" name="adsb_breach_h" type="text" /></td>
              <td>KM</td>
            </tr>
            <tr>
              <td>Define Vertical Seperation</td>
              <td><input id="adsb_breach_v" class="spinner" name="adsb_breach_v" type="text" /></td>
              <td>KM</td>
            </tr>
          </table>
        </td>
      </tr>
    </table>



    <table class="info-tiles top-space breach" cellpadding="0" cellspacing="0">
      <tr>
        <td>
          <div class="tile">
            <div class="tile-title">Breach</div>
            <div class="tile-detail">
              <table cellpadding="0" cellspacing="0">
                <tr>
                  <td>
                    <div id="breach-rpas" class="tile-number">1</div>
                    <div class="tile-caption">RPAS</div>
                  </td>
                  <td>
                    <div id="breach-aircraft" class="tile-number">4</div>
                    <div class="tile-caption">Aircraft</div>
                  </td>
                </tr>
              </table>
            </div>
          </div>

        </td>
        <td>
          <div class="detail">
            <table class="scroll-table" cellpadding="0" cellspacing="0">
              <thead>
                <tr>
                  <th>EPOCH TIME</th>
                  <th>MODE S CODE</th>
                  <th>LATITUDE</th>
                  <th>LONGITUDE</th>
                  <th>GROUNDSPEED</th>
                  <th>HEADING</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                </tr>
                <tr>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                </tr>
                <tr>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                </tr>
                <tr>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                </tr>
                <tr>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                  <td>1001</td>
                </tr>
              </tbody>
            </table>
            <div class="breach-info">Red Alerts with Aircrafts <span id="breach-aircraft">B0125, A49D2, B49A3, D8S82</span></div>
          </div>

        </td>
      </tr>


    </table>


    <table class="info-tiles top-space alert" cellpadding="0" cellspacing="0">
      <tr>
        <td>


          <div class="tile">
            <div class="tile-title">Alert</div>
            <div class="tile-detail">
              <table cellpadding="0" cellspacing="0">
                <tr>
                  <td>
                    <div id="breach-rpas" class="tile-number">1</div>
                    <div class="tile-caption">RPAS</div>
                  </td>
                  <td>
                    <div id="breach-aircraft" class="tile-number">4</div>
                    <div class="tile-caption">Aircraft</div>
                  </td>
                </tr>
              </table>
            </div>
          </div>
        </td>
        <td>
          <div class="detail_left">
            <div><b>Mode S Code</b></div>
            <div id="alert-mode-s" class="alert-text">
              BD789C<br />
              UD73LJ
            </div>
          </div>
          <div class="detail_right">
            <div><b>Amber alert with Aircraft</b></div>
            <div id="alert-aircraft" class="alert-text">
              BD789C, UD73LJ, B0125, A49D2, B49A3, D8S82
            </div>
          </div>
        </td>
      </tr>


    </table>

    <table class="info-tiles top-space safe" cellpadding="0" cellspacing="0">
      <tr>
        <td valign="top" class="safe">

          <div class="tile">
            <div class="tile-title">Safe Ops.</div>
            <div class="tile-detail">

              <div id="breach-aircraft" class="tile-number">16</div>
              <div class="tile-caption">Aircraft</div>

            </div>
          </div>
        </td>
        <td class="shaded" valign="top">
          <div class="title"><b>Mode S Codes</b> (Operating Safe RPAS)</div>
          <div id="safe-aircraft" class="alert-text">
            BD789C, UD73LJ, B0125, A49D2, B49A3, D8S82
          </div>
        </td>
        <td class="spacer"></td>
        <td class="shaded" valign="top">
          <div class="title"><b>Current Monitoring Stats</b></div>
          <table class="safe-status" cellpadding="0" cellspacing="0">
            <tr>
              <td>Broadcasting RPAS</td>
              <td><div class="label">19</div></td>
            </tr>
            <tr>
              <td>Geographical Range</td>
              <td><div class="label">585</div> SQ. KM&nbsp;</td>
            </tr>
            <tr>
              <td>Brech Alerts (24 H)</td>
              <td><div class="label">2</div></td>
            </tr>

          </table>
        </td>
      </tr>


    </table>



  </div>
        <div id="adsb_map" class="column column-half shaded"></div>
</section>


@section scripts {
  <script src="~/jwplayer/jwplayer.js" type="text/javascript"></script>
  <script src="http://code.highcharts.com/stock/highstock.js"></script>
  <script src="https://code.highcharts.com/modules/exporting.js"></script>
<script src="~/dataTable/js/jquery.dataTables.js"></script>
  <script type="text/javascript" src="//maps.google.com/maps/api/js?key=AIzaSyDY6IHXH4-fUAzYq6HBR_ndu_lnyyopcTs"></script>
  <script src="~/Scripts/ADSB.js" type="text/javascript"></script>
  <script src="~/Scripts/ADSBDashboard.js" type="text/javascript"></script>
}